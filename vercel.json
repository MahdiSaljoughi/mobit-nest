{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "prisma/schema.prisma",
      "use": "@vercel/node",
      "buildCommand": "npx prisma generate"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
